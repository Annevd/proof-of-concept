<%- include('./partials/head') %>

<% products.forEach(product => { %>
    <div>
        <h2><%= product.title %></h2>
        <% if (product.imageUrl) { %>
            <img src="<%= product.imageUrl %>" alt="<%= product.title %>" width="400" height="266">
        <% } %>
        <p><%= product.description %></p>
    </div>
<% }) %>

<%- include('./partials/foot') %>
