
<body>
<main class="desktopSpacing">
    <h1>Articles</h1>
    <ul>
        <% articles.forEach(article => { %>
            <li>
                <h2><%= article.title %></h2>
                <p><%= article.introduction %></p>
                <% if (article.photo) { %>
                    <img src="<%= article.photo.URL %>" alt="Article Image">
                <% } %>
            </li>
        <% }) %>
    </ul>
</main>
</body>
